Algoritmo "consumo medio"

Var
   distancia : inteiro
   combustivelGasto: real
   consumoMedio: real



Inicio
   escreva ("Distancia Percorida:")
   leia (distancia)
   escreva ("Combustivel gasto:")
   leia (combustivelGasto)

   consumoMedio <- distancia / combustivelGasto
   escreval ("Consumo medio = " , consumoMedio: 8:3)


Fimalgoritmo